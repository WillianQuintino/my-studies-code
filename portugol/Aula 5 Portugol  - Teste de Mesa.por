programa {
  funcao inicio() {
    inteiro a, b, soma, sub, mult, div

    escreva("Digite o valor de a:")
    leia(a)

    escreva("Digite o valor de b:")
    leia(b)

    soma = a + b
    sub = a - b
    mult = a * b
    div = a / b


    escreva("\nsoma = ", soma)
    escreva("\nsubtração = ", sub)
    escreva("\ndivisão = ", div)
    escreva("\nmultiplicação = ", mult)
  }
}
